<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#CFD2D3" >
	<RelativeLayout
	    android:layout_width="match_parent"
	    android:layout_height="43dp"
	    android:background="@drawable/top_bar"
	    android:orientation="vertical" >
	    <LinearLayout
	         android:layout_width="40dp"
	         android:layout_height="30dp"
	         android:layout_alignParentLeft="true"
	         android:layout_centerVertical="true"
	         android:layout_marginLeft="7dp"
	         android:id="@+id/back_line"
	         android:background="@drawable/button_top">
	     <ImageButton 
	        android:layout_width="30dp"
	        android:layout_height="25dp"
	        android:layout_alignParentLeft="true"
	        android:layout_gravity="center"
	        android:scaleType="fitXY"
	        android:layout_marginTop="2dp"
	        android:layout_marginLeft="5dp"
	        android:background="@drawable/back" 
	        android:id="@+id/back"
	        />
	     </LinearLayout>
	     <TextView
	           style="@style/title"
	           android:textIsSelectable="true"
	           android:ellipsize="end"
	           android:singleLine="true"
	           android:layout_width="wrap_content"
	           android:scrollHorizontally="true"
	           android:focusable="true"
	           android:text="@string/change_user"
	           />
	     <LinearLayout
	         android:layout_width="40dp"
	         android:layout_height="30dp"
	         android:id="@+id/choose_visible"
	         android:layout_alignParentRight="true"
	         android:layout_centerVertical="true"
	         android:layout_marginRight="7dp"
	         android:background="@drawable/button_top">
	        <ImageButton
	            android:layout_width="30dp"
	            android:layout_height="25dp"
	            android:scaleType="fitXY"
	            android:id="@+id/add_account"
	            android:layout_gravity="center"
	            android:layout_marginTop="2dp"
	            android:layout_marginLeft="4dp"
	            android:background="@drawable/nav_add_icon"
	            />
	     </LinearLayout>
	</RelativeLayout> 
	<TextView 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/dont_interept"
	    android:layout_marginLeft="15dp"
	    android:layout_marginTop="15dp"
	    android:textColor="#000000"
	    android:textSize="13sp"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="10dp"
	    android:layout_marginRight="10dp"
	    android:layout_marginTop="10dp"
	    android:background="@drawable/account_top_bg"
	    android:orientation="horizontal"
	    >
		<TextView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="@string/dont_interept_1"
		    android:layout_gravity="center_vertical"
		    android:layout_marginLeft="5dp"
		    />
		<TextView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:id="@+id/interupt_time"
		    android:textIsSelectable="true"
		    android:text="每日23：00-次日12：20"
		    android:layout_gravity="center_vertical"
		    android:layout_marginLeft="30dp"/>
	</LinearLayout>
	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:id="@+id/feedback"
	    android:layout_marginLeft="10dp"
	    android:layout_marginRight="10dp"
	    android:background="@drawable/account_bottom_bg"
	    android:orientation="horizontal"
	    >
		<TextView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="@string/feed_back"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="5dp"
		    android:layout_alignParentLeft="true"
		    />
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_alignParentRight="true"
		    android:background="@drawable/ic_media_play"
		    android:layout_marginRight="10dp"
		    />
	</RelativeLayout>
	
	<!-- 2 -->
<TextView 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="@string/push_set"
	    android:layout_marginLeft="15dp"
	    android:layout_marginTop="10dp"
	    android:textColor="#000000"
	    android:textSize="13sp"/>
	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:id="@+id/feedback"
	    android:layout_marginLeft="10dp"
	    android:layout_marginRight="10dp"
	    android:layout_marginTop="15dp"
	    android:background="@drawable/account_top_bg"
	    android:orientation="horizontal"
	    >
		<TextView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="@string/single_day_data"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="5dp"
		    android:layout_alignParentLeft="true"
		    />
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_alignParentRight="true"
		    android:background="@drawable/ic_media_play"
		    android:layout_marginRight="10dp"
		    />
	</RelativeLayout>
	<RelativeLayout 
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:id="@+id/feedback"
	    android:layout_marginLeft="10dp"
	    android:layout_marginRight="10dp"
	    android:background="@drawable/account_bottom_bg"
	    android:orientation="horizontal"
	    >
		<TextView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="@string/service_update_note"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="5dp"
		    android:layout_alignParentLeft="true"
		    />
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_alignParentRight="true"
		    android:background="@drawable/ic_media_play"
		    android:layout_marginRight="10dp"
		    />
	</RelativeLayout>
</LinearLayout>